<?xml version="1.0"?>
<project version="2.0">
<var name="remap">"[RadiantPath]remap.[ExecutableType]" -v<cond value="[MonitorAddress]"> -connect [MonitorAddress]</cond> -game titanfall2 -fs_basepath "[EnginePath]"<cond value="[GameName]"> -fs_game [GameName]</cond></var>
<build name="Simple BSP">
<command>[remap] -meta "[MapFile]"</command>
</build>
<build name="Only entities (Doesn't modify the map!)">
<command>[remap] -onlyents -meta "[MapFile]"</command>
</build>

